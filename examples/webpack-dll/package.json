{
  "name": "webpack-demo",
  "version": "0.0.1",
  "description": "webpack4 demo with vue&eslint&babel&stylus&postcss",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "build": "npm run dll && npm run prod",
    "dll": "NODE_ENV=production npx webpack --config webpack.dll.js",
    "prod": "NODE_ENV=production npx webpack",
    "serve": "npx http-server -c0 -p 1234 ./output & npx http-server -c0 -o -p 8000 ./dist"
  },
  "keywords": [
    "webpack",
    "babel",
    "eslint",
    "postcss",
    "stylus",
    "vue"
  ],
  "author": "babytutu",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.28.10",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^4.7.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "mini-css-extract-plugin": "^0.4.5",
    "mkdirp": "^0.5.1",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "postcss-preset-env": "^5.2.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.5"
  },
  "dependencies": {
    "vue": "^2.5.21",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1"
  }
}
