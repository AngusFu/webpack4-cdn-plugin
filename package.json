{
  "name": "webpack4-cdn-plugin",
  "version": "1.3.9",
  "main": "index.js",
  "description": "Upload your webpack-generated assets to CDN, allowing renaming/rehashing.",
  "keywords": [
    "webpack",
    "webpack4",
    "CDN",
    "upload",
    "rename"
  ],
  "files": [
    "./index.js"
  ],
  "repository": "git+https://github.com/AngusFu/webpack4-cdn-plugin.git",
  "author": "wemlion <angusfu1126@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AngusFu/webpack4-cdn-plugin/issues"
  },
  "homepage": "https://github.com/AngusFu/webpack4-cdn-plugin#readme",
  "peerDependencies": {
    "webpack": "4.*"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "webpack-sources": "^1.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "babel-eslint": "^10.0.1",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-formatter-friendly": "^6.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "standard-version": "^5.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "eslint --format friendly"
      ]
    },
    "ignore": [
      "**/dist/*.js"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "scripts": {
    "commit": "npx git-cz",
    "release": "npx standard-version",
    "lint": "eslint ./",
    "lint:fix": "eslint --fix --ext .js,.jsx ./",
    "format": "pretty-quick --pattern \"**/*.*(js|jsx)\""
  }
}
